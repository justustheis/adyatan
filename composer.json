{
    "name": "justustheis/adyatan",
    "description": "Adyatan provides a convenient way to update Laravel applications via git.",
    "license": "MIT",
    "authors": [
        {
            "name": "Justus Theis",
            "email": "j.theis@ds-systems.eu",
            "homepage": "https://j-theis.de"
        },
        {
            "name": "Neha Sharma"
        }
    ],
    "homepage": "https://github.com/justustheis/adyatan",
    "keywords": ["Laravel", "Adyatan", "Updater", "Artisan", "CLI"],
    "require": {
        "illuminate/support": "^6.6|^7|^8",
		"illuminate/console": "^6.8|^7|^8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4|~5.0|~6.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "JustusTheis\\Adyatan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JustusTheis\\Adyatan\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JustusTheis\\Adyatan\\AdyatanServiceProvider"
            ],
            "aliases": {
                "Adyatan": "JustusTheis\\Adyatan\\Facades\\Adyatan"
            }
        }
    }
}
